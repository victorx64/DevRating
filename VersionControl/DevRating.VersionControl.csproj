<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <LangVersion>8</LangVersion>
        <Nullable>enable</Nullable>
        <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
        <PackageOutputPath>../Package</PackageOutputPath>
        <IsPackable>true</IsPackable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>4.0.0</PackageVersion>
        <Authors>Victor Semenov</Authors>
        <Title>DevRating.VersionControl</Title>
        <PackageProjectUrl>https://devrating.net</PackageProjectUrl>
        <RepositoryUrl>https://github.com/victorx64/devrating</RepositoryUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Description>Version Control System types</Description>
        <Copyright>Copyright © Viktor Semenov 2019-present</Copyright>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="semver" Version="2.0.6"/>
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\DefaultObject\DevRating.DefaultObject.csproj"/>
    </ItemGroup>

</Project>
